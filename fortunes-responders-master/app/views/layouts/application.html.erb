<!doctype html>
<html lang="pl">
  <head>
    <meta charset="utf-8">
    <title><%= content_for?(:title) ? yield(:title) : "Responders" %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <%= auto_discovery_link_tag(:atom, fortunes_path(:atom)) %>

    <%= stylesheet_link_tag "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>

    <!--[if lt IE 9]>
    <script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>

  <body>

    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <%= render :partial => 'common/menu' %>
        </div>
      </div>
    </div>

    <div class="container">
      <%= render :partial => 'common/flashes' %>
      <%= content_tag :h1, yield(:title) if show_title? %>
      <%= yield %>
<div class="digg_pagination">
  <div class="page_info">
    <%= page_entries_info @fortunes %>
  </div>
  <%= will_paginate @fortunes, :container => false %>
</div>



      <footer>
        <%= render :partial => 'common/footer' %>
      </footer>
    </div>

  </body>
</html>

